@model IEnumerable<Domain.MedicalPath>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout1.cshtml";
}


<div class="box-header">
    <h3 class="box-title"> Liste  Parcour Patient</h3>
</div>

<div class="box">
    <div class="box-header">
    </div><!-- /.box-header -->
    <div class="box-body">
        <table id="example1" class="table table-bordered table-striped">
            <thead>
                <div>
                    @Html.ActionLink("Go Back ", "listPatient")
                </div>
                @*<input type="text" value="@ViewBag.UserId" id="submit" />*@
                    <tr>
                        <th>
                            @*<input type="submit" value="Search" id="submit" />*@
                            @Html.DisplayNameFor(model => model.Description)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.DateParcour)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.DoctorId)
                        </th>
                        @*<th>
                                @Html.DisplayNameFor(model => model.AdresseDotor)
