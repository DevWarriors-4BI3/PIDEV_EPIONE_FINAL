                            </th>*@
                        <th>
                            @Html.DisplayNameFor(model => model.SpecialityId)
                        </th>
                        <th></th>
                    </tr>

                    @foreach (var item in Model)
                    {
                        <tr>
                            <td class="danger">
                                @Html.DisplayFor(modelItem => item.Description)
                            </td>
                            <td class="danger">
                                @Html.DisplayFor(modelItem => item.DateParcour)
                            </td>
                            <td class="danger">
                                @Html.DisplayFor(modelItem => item.DoctorId)
                            </td>

                            <td class="danger">
                                @Html.DisplayFor(modelItem => item.SpecialityId)
                                @*@Html.DisplayFor(@Service.ServiceSpeciality.GetNomById(modelItem => item.SpecialityId))*@
                            </td>
                            <td class="danger">
                                @Html.ActionLink("Edit", "Edit", new { id = item.MedicalPathId }) |
                                @Html.ActionLink("Details", "Details", new { id = item.MedicalPathId }) |
                                @Html.ActionLink("Delete", "Delete", new { id = item.MedicalPathId })
                            </td>
                        </tr>
                    }
                     </tbody>

